N = int(input('введите номер квартиры '))
pdV = int(input('введите кол-во подъездов '))
etV = int(input('введите кол-во этажей '))
kvV = int(input('введите кол-во квартир на этаже '))
pd = int() # подъезд
et = int() # этаж
if N%(kvV*etV) !=0: # проверка кратности кол-ва квартир в подъезде
    pd = N//(kvV*etV)+1 # Вариант_1 (если не последняя квартира в подъезде)
else:
    pd = N // (kvV * etV) # Вариант_2 (если последняя квартира в подъезде)
N=N-(pd-1)*kvV*etV # выделяем наш подъед
if N%kvV!=0: # проверка кратности кол-ва квартир на этаже
    et=N//kvV+1 # Вариант_1
else:
    et = N // kvV# Вариант_2
N=N-(et-1)*kvV # выделяем порядковый номер квартиры на этаже
if pd>pdV:# проверка существования квартиры в доме
    print("В данном доме нет такой квартиры")
else:
    print ("Подъезд  ", pd)
    print("Этаж  ", et)
    print("Номер на этаже ", N)
